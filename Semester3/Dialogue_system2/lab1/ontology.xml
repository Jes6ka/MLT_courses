<?xml version="1.0" encoding="utf-8"?>
<ontology name="HelloWorldOntology">

<sort name="alarm"/>

<predicate name="hour_to_set" sort="integer"/>
<predicate name="minute_to_set" sort="integer"/>

<predicate name="current_time" sort="string"/>
<predicate name="alarm_time" sort="string"/>
<predicate name="current_time_oneshot" sort="string"/>
<predicate name="alarm_time_oneshot" sort="string"/>

<action name="top"/>
<action name="set_time"/>
<action name="set_alarm"/>
<action name="set_time_oneshot"/>
<action name="set_alarm_oneshot"/>

</ontology>
